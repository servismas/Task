@model IEnumerable<TaskShedulerMVC.BLL.ModelsDTO.TaskShedulDTO>

<div class="row">
    @foreach (var t in Model)
    {
        <div class="col-xl-3 col-lg-4 col-md-6 col-12 game-card">
            <div class="card">

                <div class="card-header">
                    <p>@t.Deadline</p>
                </div>

                <div class="card-body">
                    <p>@t.Deskription</p>
                    <p>@t.Categories.ElementAt(0).Name</p> @*добавить цикл*@
                </div>

                <div class="card-footer">
                    <button class="btn btn-success w-100">Edit</button>
                </div>
            </div>

        </div>
    }


</div>
